    <CustomFolders>
        <Folder
            Name="Source Files"
            Filters="*.c;*.C;*.cc;*.cpp;*.cp;*.cxx;*.c++;*.prg;*.pas;*.dpr;*.asm;*.s;*.bas;*.java;*.cs;*.sc;*.scala;*.e;*.cob;*.html;*.rc;*.tcl;*.py;*.pl;*.d;*.m;*.mm;*.go;*.groovy;*.gsh"
            GUID="{B135E13E-B5E4-435B-8034-0E4CDE736366}"></Folder>
        <Folder
            Name="Header Files"
            Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.h++;*.inc;*.sh;*.cpy;*.if"
            GUID="{F3E52624-4456-47CD-8A12-3F7A2DE77359}"/>
        <Folder
            Name="Resource Files"
            Filters="*.ico;*.cur;*.dlg"
            GUID="{9A3F53E3-935A-45C0-A62A-B1B2F64D3816}"/>
        <Folder
            Name="Bitmaps"
            Filters="*.bmp"
            GUID="{83203ABC-902C-45D4-A5CC-3EAAFB6A3DAD}"/>
        <Folder
            Name="Other Files"
            Filters=""
            GUID="{85CE65E1-131F-44A2-A621-6A04E86E6E61}"/>
    </CustomFolders>
    <Files AutoFolders="DirectoryView">
        <F N="main.cpp"/>
    </Files>
</Project>
